{
    "name": "henryhayes/merge",
    "type": "magento-module",
    "authors": [
        {
            "name": "Henry Hayes",
            "email": "henryhayesuk@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require":{
        "aydin-hassan/magento-core-composer-installer": "~2.0.0",
        "magento-hackathon/magento-composer-installer":"*",
        "openmage/magento-lts": "^19.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload-dev": {
        "psr-0": {
            "HenryHayes": "src/app/code/community"
        }
    },
    "extra": {
        "map": [
            ["app/code/community/HenryHayes/Merge", "app/code/community/HenryHayes"],
            ["app/etc/modules/HenryHayes_Merge.xml", "app/etc/modules/HenryHayes_Merge.xml"]],
        "magento-root-dir": "./"
    },
    "config": {
        "allow-plugins": {
            "magento-hackathon/magento-composer-installer": true,
            "cweagans/composer-patches": true,
            "aydin-hassan/magento-core-composer-installer": true
        }
    }
}
